{% extends "partials/page_head.html.twig" %}

{% block head_application -%}
    <meta charset="utf-8" />
    <meta http-equiv="content-type" content="text/html; charset=utf-8" />
    <title>{{ errorcode|default(500) }} {{ error|default('GANTRY5_ENGINE_UNKNOWN_ERROR'|trans) }}</title>
    {% do gantry.document.errorPage(true) -%}
    {% do gantry.platform.finalize() -%}
    {{ gantry.styles('head', true)|join("\n    ")|raw }}
	<script src="{{ gantry.jf_g5_theme.joomla_root_true() }}/media/jui/js/jquery.min.js" type="text/javascript"></script>
	<script src="{{ gantry.jf_g5_theme.joomla_root_true() }}/media/jui/js/jquery-noconflict.js" type="text/javascript"></script>
	<script src="{{ gantry.jf_g5_theme.joomla_root_true() }}/media/jui/js/jquery-migrate.min.js" type="text/javascript"></script>
    {{ gantry.scripts('head', true)|join("\n    ")|raw }}
	<script src="{{ gantry.jf_g5_theme.tmpl_root() }}jf/features/jf_revealer/anime.min.js" type="text/javascript"></script>
	<script src="{{ gantry.jf_g5_theme.tmpl_root() }}jf/features/jf_revealer/revealer.min.js" type="text/javascript"></script>
	<script src="{{ gantry.jf_g5_theme.tmpl_root() }}jf/assets/js/jf.min.js" type="text/javascript"></script>
{%- endblock %}
